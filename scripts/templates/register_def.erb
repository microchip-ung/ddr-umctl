#ifndef __REGISTER_DEF_H
#define __REGISTER_DEF_H

#include <stdint.h>

<%- soc.chip.targets.each do |t| -%>
<%- t[:groups].each do |g| -%>
<%- g[:registers].each do |r| -%>
#define <%= sprintf("%-20s 0x%08x", t[:name].upcase + "_" + r[:name].upcase, soc.chip.regaddr(t, g, r)) %>
<%- end -%>
<%- end -%>
<%- end -%>

#endif /* __REGISTER_DEF_H */
