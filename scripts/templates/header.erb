#ifndef _DDR_CONFIG_H
#define _DDR_CONFIG_H

#include <stdint.h>

struct ddr_config {
<%- config.groups.each do |rg| -%>
    struct {
<%- rg[:regs].sort.each do |rname| -%>
        uint32 <%= rname %>;
<%- end -%>
    } <%= rg[:name] %>;
<%- end -%>
};

#endif /* _DDR_CONFIG_H */
